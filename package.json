{
  "name": "team-shinchan",
  "version": "2.5.0",
  "description": "Shinchan character-based multi-agent orchestration system for Claude Code",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "bun build src/index.ts --outdir dist --target node",
    "dev": "bun run --watch src/index.ts",
    "test": "bun test",
    "lint": "eslint src/",
    "typecheck": "tsc --noEmit",
    "install-plugin": "bun run build && cp -r dist ~/.claude/plugins/team-shinchan/",
    "sync-version": "./scripts/sync-version.sh",
    "release": "npm run sync-version && git add -A && git commit -m \"chore: bump version to $(node -p \"require('./package.json').version\")\" && git push origin main"
  },
  "keywords": [
    "claude-code",
    "plugin",
    "multi-agent",
    "orchestration",
    "team-shinchan",
    "shinchan"
  ],
  "author": "seokan",
  "license": "MIT",
  "dependencies": {
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "@types/uuid": "^9.0.0",
    "typescript": "^5.3.0"
  },
  "peerDependencies": {
    "typescript": "^5.0.0"
  }
}
